{
    "name": "pimcore/installer-areabrick",
    "type": "composer-installer",
    "license": "BSD-3-Clause",
	"description": "A custom composer installer for pimcore - area bricks",
	"keywords": ["pimcore","area brick","installer"],
	"homepage": "http://www.pimcore.org/",
	"authors": [
        {
            "name": "pimcore Team",
            "email": "info@pimcore.com",
            "homepage": "http://www.pimcore.org/",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-0": {"Pimcore\\Composer\\": "src/"}
    },
    "extra": {
        "class": "Pimcore\\Composer\\AreabrickInstaller"
    },
	"require": {
        "pimcore/pimcore": ">=2"
    }
}